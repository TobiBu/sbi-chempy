	eprint = {1703.02970},
	journal = {\mnras},
	keywords = {methods: numerical, galaxies: evolution, galaxies: formation},
	month = jan,
	pages = {4077-4106},
	title = {{Simulating galaxy formation with the IllustrisTNG model}},
